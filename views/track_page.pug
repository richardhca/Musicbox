#track_page_detail
    #track_page_track_list.list-group
        .list-group-item
            .row.flex-nowrap
                .col(id="tracklist_icon_title")
                .col
                    .text-white.noselect Track
                .col
                    .text-white.noselect Artist
                .col
                    .text-white.noselect Album
                .col
                    .text-white.noselect Genres
                .col
                    .text-white.noselect Duration
        for track in tracks
            - const track_name = (null === track.title ? 'None' : track.title)
            - const artist_name = (undefined === track.artist ? 'None' : track.artist)
            - const album_name = (undefined === track.album ? 'None' : track.album)
            - const genres_name = (null === track.genres ? 'None' : track.genres)
            - const duration = (undefined === track.duration ? 'None' : track.duration)
            a.list-group-item.track_list
                .row.flex-nowrap
                    .col.tracklist_icon
                        i.material-icons.text-white.noselect music_note
                    .col
                        .text-white.noselect #{track_name}
                    .col
                        .text-white.noselect #{artist_name}
                    .col
                        .text-white.noselect #{album_name}
                    .col
                        .text-white.noselect #{genres_name}
                    .col
                        .text-white.noselect #{duration}


